{
    "manifest_version": 2,
    "name": "Ignore Video Clicks",
    "description": "Ignores clicks on video.",
    "version": "1.0",
    "author": "Sen Palanisami",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "128": "icon128.png",
        "512": "icon512.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [ "content_script.js" ],
            "run_at": "document_end"
        }
    ]
}
